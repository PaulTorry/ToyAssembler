  defn muliplier
  defn numberOfLoops
  defn total
  inp 5  
  sto muliplier
  inp 0
  sto numberOfLoops
  inp 3
  sto total
  
  label looper
  ret total
  goif done
  ret numberOfLoops
  add muliplier
  sto numberOfLoops
  dec total  
  goto looper
  
  label done
  ret numberOfLoops
  prt 
  end 





increment
  defn r0
  defn r1
  inp 7  
  sto r1
  inc r1
  prt 
  end


